{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-project-df"
		},
		"ls_covid_data_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidinfodl.dfs.core.windows.net/"
		},
		"ls_covid_data_dl_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "e9afc002-31c1-47b3-962c-99ac507abdf2"
		},
		"ls_covid_data_dl_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "0b22f273-b431-414f-a1bf-507524a0dea5"
		},
		"ls_covid_data_web_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu"
		},
		"ls_covid_project_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://covid-project-kv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-project-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covid_data_web_source_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_data_web_source_properties_typeProperties_url')]"
					},
					"ls_covid_project_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_covid_project_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_covid_data_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_url')]"
					},
					"ls_covid_data_dl_properties_typeProperties_tenant": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_tenant')]"
					},
					"ls_covid_data_dl_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}