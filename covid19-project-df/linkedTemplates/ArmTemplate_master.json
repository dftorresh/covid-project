{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-project-df"
		},
		"ls_processed_data_sql_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_processed_data_sql_db'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sqlservercovidproject28.database.windows.net;Initial Catalog=covid_data;User ID=aduser"
		},
		"ls_covid_data_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid28datadl.dfs.core.windows.net/"
		},
		"ls_covid_data_dl_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "e9afc002-31c1-47b3-962c-99ac507abdf2"
		},
		"ls_covid_data_dl_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "87c40ddb-5c63-44e6-b7bd-6409e8142f6c"
		},
		"ls_covid_data_web_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu"
		},
		"ls_covid_project_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-covid-project28.vault.azure.net/"
		},
		"ls_databricks_covid_project_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0103-030730-wiwtps2a"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-project-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_processed_data_sql_db_connectionString": {
						"value": "[parameters('ls_processed_data_sql_db_connectionString')]"
					},
					"ls_covid_data_web_source_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_data_web_source_properties_typeProperties_url')]"
					},
					"ls_covid_project_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_covid_project_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_covid_data_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_url')]"
					},
					"ls_covid_data_dl_properties_typeProperties_tenant": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_tenant')]"
					},
					"ls_covid_data_dl_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_covid_data_dl_properties_typeProperties_servicePrincipalId')]"
					},
					"ls_databricks_covid_project_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_covid_project_ws_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}