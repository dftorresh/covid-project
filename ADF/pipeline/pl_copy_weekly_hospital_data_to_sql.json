{
	"name": "pl_copy_weekly_hospital_data_to_sql",
	"properties": {
		"activities": [
			{
				"name": "copy_process_weekly_data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "weekly_data",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE [dbo].[weekly_hospital_admissions];",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "country",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "country",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "country_code_2_digit",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "country_code_2_digit",
									"type": "String",
									"physicalType": "char"
								}
							},
							{
								"source": {
									"name": "country_code_3_digit",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "country_code_3_digit",
									"type": "String",
									"physicalType": "char"
								}
							},
							{
								"source": {
									"name": "population",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "population",
									"type": "Int64",
									"physicalType": "bigint"
								}
							},
							{
								"source": {
									"name": "reported_year_week",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "year_week",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "reported_week_start_date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "week_start_date",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "reported_week_end_date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "week_end_date",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "new_hospital_occupancy_count",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "hospital_occupancy_count",
									"type": "Single",
									"physicalType": "real"
								}
							},
							{
								"source": {
									"name": "new_icu_occupancy_count",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "icu_occupancy_count",
									"type": "Single",
									"physicalType": "real"
								}
							},
							{
								"source": {
									"name": "source",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "testing_data_source",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_processed_weekly_covid_data_csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_weekly_hospital_data_sql_table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}